local Library = loadstring(game:HttpGet("https://pastebin.com/raw/vff1bQ9F"))()
local Window = Library.CreateLib("LUCKY BLOCKS Battlegrounds", "Ocean")


local Tab = Window:NewTab("Credits")



local Section = Tab:NewSection("Maked by EpicGamerHero_Shoots7")

local Tab = Window:NewTab("Main")


local Section = Tab:NewSection("Tap to get a weapon")


Section:NewButton("Luckyblock", "ButtonInfo", function()
game:GetService("ReplicatedStorage"):WaitForChild("SpawnLuckyBlock"):FireServer()

    print("Clicked")
end)


