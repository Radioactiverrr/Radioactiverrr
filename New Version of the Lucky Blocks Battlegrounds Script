local Library = loadstring(game:HttpGet("https://pastebin.com/raw/vff1bQ9F"))()
local Window = Library.CreateLib("LUCKY BLOCKS Battlegrounds Newer Version", "Ocean")


local Tab = Window:NewTab("Updates")


local Section = Tab:NewSection("Added A player Tab")
 
 
local Tab = Window:NewTab("Credits")
 
 
 
local Section = Tab:NewSection("Maked by EpicGamerHero_Shoots7")
 
local Tab = Window:NewTab("Main")
 
 
local Section = Tab:NewSection("Tap to get a weapon")
 
 
Section:NewButton("Luckyblock", "ButtonInfo", function()
game:GetService("ReplicatedStorage"):WaitForChild("SpawnLuckyBlock"):FireServer()
 
    print("Clicked")
end)
 
 
Section:NewButton("SuperBlock", "ButtonInfo", function()
game:GetService("ReplicatedStorage"):WaitForChild("SpawnSuperBlock"):FireServer()
 
    print("Clicked")
end)
 
 
Section:NewButton("DiamondBlock", "ButtonInfo", function()
game:GetService("ReplicatedStorage"):WaitForChild("SpawnDiamondBlock"):FireServer()
 
    print("Clicked")
end)
 
 
Section:NewButton("RainbowBlock", "ButtonInfo", function()
game:GetService("ReplicatedStorage"):WaitForChild("SpawnRainbowBlock"):FireServer()
 
    print("Clicked")
end)
 
 
Section:NewButton("GalaxyBlock", "ButtonInfo", function()
game:GetService("ReplicatedStorage"):WaitForChild("SpawnGalaxyBlock"):FireServer()
 
    print("Clicked")
end)
 
 
local Tab = Window:NewTab("Player")
 
 
local Section = Tab:NewSection("Player")


Section:NewSlider("Walkspeed", "SliderInfo", 1000, 0, function(s) -- 500 (MaxValue) | 0 (MinValue)
    game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = s
end)


Section:NewSlider("Jumppower", "SliderInfo", 500, 0, function(s) -- 500 (MaxValue) | 0 (MinValue)
    game.Players.LocalPlayer.Character.Humanoid.JumpPower = s
end)


